# Variables de Entorno - Desarrollo
# Copiar este archivo a .env.local y configurar las variables

# API Base URL
VITE_API_BASE_URL=http://localhost:3000/api

# Entorno
VITE_APP_ENV=development

# Nombre de la aplicación
VITE_APP_NAME=InspiraTec

# URL de la aplicación
VITE_APP_URL=http://localhost:5173

# Configuración de autenticación
VITE_TOKEN_EXPIRATION=86400000

# Configuración de Google OAuth (opcional)
VITE_GOOGLE_CLIENT_ID=

# Configuración de Microsoft OAuth (opcional)
VITE_MICROSOFT_CLIENT_ID=

# Sentry DSN para tracking de errores (opcional)
VITE_SENTRY_DSN=

# Analytics ID (opcional)
VITE_ANALYTICS_ID=
